<div class="d">
<h2>Log in</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :email, class: "actions" %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "d" %>
  </div>

  <div class="field">
    <%= f.label :password, class: "actions" %><br />
    <%= f.password_field :password, autocomplete: "current-password", class: "d" %>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="field">
      <%= f.check_box :remember_me, class: "actions" %>
      <%= f.label :remember_me %>
    </div>
  <% end %>

  <div class="actions">
    <%= f.submit "Log in", class: "d" %>
  </div>
<% end %>

<div class= "actions">
<%= render "devise/shared/links" %>
</div>

